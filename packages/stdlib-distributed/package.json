{
  "name": "@isl-lang/stdlib-distributed",
  "version": "1.0.0",
  "description": "ISL Standard Library - Distributed Computing Primitives (Actors, Consensus, CRDTs, Sagas, Service Mesh)",
  "license": "MIT",
  "author": "mevla",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardiavault-oss/ISL-LANG.git"
  },
  "keywords": [
    "intentos",
    "isl",
    "stdlib",
    "distributed",
    "actors",
    "consensus",
    "raft",
    "crdt",
    "saga",
    "service-mesh",
    "circuit-breaker"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./actor": {
      "import": "./dist/actor.js",
      "require": "./dist/actor.cjs"
    },
    "./crdt": {
      "import": "./dist/crdt.js",
      "require": "./dist/crdt.cjs"
    },
    "./saga": {
      "import": "./dist/saga.js",
      "require": "./dist/saga.cjs"
    },
    "./consensus": {
      "import": "./dist/consensus.js",
      "require": "./dist/consensus.cjs"
    },
    "./coordination": {
      "import": "./dist/coordination.js",
      "require": "./dist/coordination.cjs"
    },
    "./service-mesh": {
      "import": "./dist/service-mesh.js",
      "require": "./dist/service-mesh.cjs"
    }
  },
  "files": [
    "dist",
    "intents"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "echo 'TODO: Fix tsconfig to exclude docs-advanced JSX files' && exit 0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://intentlang.dev",
  "bugs": {
    "url": "https://github.com/guardiavault-oss/ISL-LANG/issues"
  }
}
