{
  "name": "@isl-lang/compliance",
  "version": "0.1.1",
  "description": "Generate compliance reports (SOC2, PCI-DSS, HIPAA, GDPR, EU AI Act, FedRAMP) from ISL specifications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "test:unit": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@isl-lang/isl-core": "workspace:*",
    "@isl-lang/isl-verify": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "vitest": "^1.2.0",
    "tsup": "^8.0.1",
    "rimraf": "^5.0.5"
  },
  "keywords": [
    "compliance",
    "soc2",
    "pci-dss",
    "hipaa",
    "gdpr",
    "eu-ai-act",
    "fedramp",
    "isl"
  ],
  "author": "mevla",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardiavault-oss/ISL-LANG.git"
  },
  "homepage": "https://intentlang.dev",
  "bugs": {
    "url": "https://github.com/guardiavault-oss/ISL-LANG/issues"
  },
  "private": true,
  "experimental": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
