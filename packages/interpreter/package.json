{
  "name": "@isl-lang/interpreter",
  "version": "1.0.0",
  "description": "ISL Runtime Interpreter - Execute ISL specs against live code/data for verification",
  "license": "MIT",
  "author": "mevla",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mevla/isl-lang.git"
  },
  "keywords": [
    "intentos",
    "isl",
    "interpreter",
    "verification",
    "contracts",
    "runtime",
    "testing"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@isl-lang/parser": "workspace:*",
    "@isl-lang/typechecker": "workspace:*",
    "@isl-lang/runtime-interpreter": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://intentlang.dev",
  "bugs": {
    "url": "https://github.com/mevla/isl-lang/issues"
  }
}
