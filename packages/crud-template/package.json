  },
  "dependencies": {},
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "tsx": "^4.7.0",
    "vitest": "^1.2.0"
  },
  "files": ["dist"],
  "license": "MIT"
