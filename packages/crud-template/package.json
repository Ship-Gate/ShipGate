{
  "name": "@isl-lang/crud-template",
  "version": "0.1.0",
  "description": "Parameterized CRUD golden template for Next.js + Prisma + TanStack Query",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
