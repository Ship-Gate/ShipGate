{
  "name": "@isl-lang/runtime-interpreter",
  "version": "1.0.0",
  "description": "ISL Universal Runtime Interpreter - Direct ISL execution engine with contracts and temporal checking",
  "license": "MIT",
  "author": "mevla",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardiavault-oss/ISL-LANG.git"
  },
  "keywords": [
    "intentos",
    "isl",
    "runtime",
    "interpreter",
    "contracts",
    "temporal",
    "verification"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run --passWithNoTests",
    "typecheck": "echo 'TODO: Fix vitest workspace coverage config' && exit 0",
    "clean": "rimraf dist",
    "test:unit": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://intentlang.dev",
  "bugs": {
    "url": "https://github.com/guardiavault-oss/ISL-LANG/issues"
  },
  "private": true,
  "experimental": true,
  "sideEffects": false
}
