{
  "name": "@isl-lang/test-runtime-legacy",
  "private": true,
  "version": "0.1.0",
  "description": "Runtime bindings for ISL generated tests - provides entity proxies and state management",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "test:unit": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0",
    "tsup": "^8.0.1"
  },
  "author": "mevla",
  "license": "MIT",
  "sideEffects": false
}
