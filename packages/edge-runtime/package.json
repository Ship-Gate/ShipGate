{
  "name": "@isl-lang/edge-runtime",
  "version": "0.1.1",
  "description": "ISL runtime for edge computing platforms (Cloudflare Workers, Deno Deploy, Vercel Edge)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cloudflare": {
      "types": "./dist/cloudflare/index.d.ts",
      "import": "./dist/cloudflare/index.js"
    },
    "./deno": {
      "types": "./dist/deno/index.d.ts",
      "import": "./dist/deno/index.js"
    },
    "./vercel": {
      "types": "./dist/vercel/index.d.ts",
      "import": "./dist/vercel/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "test:unit": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@isl-lang/verifier-runtime": "workspace:*"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "tsup": "^8.0.1",
    "rimraf": "^5.0.5"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "isl",
    "edge",
    "cloudflare",
    "workers",
    "deno",
    "vercel",
    "serverless",
    "runtime"
  ],
  "license": "MIT",
  "author": "mevla",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardiavault-oss/ISL-LANG.git"
  },
  "homepage": "https://intentlang.dev",
  "bugs": {
    "url": "https://github.com/guardiavault-oss/ISL-LANG/issues"
  },
  "private": true,
  "experimental": true,
  "sideEffects": false
}
