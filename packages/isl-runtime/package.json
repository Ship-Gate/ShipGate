{
  "name": "@isl-lang/isl-runtime",
  "version": "0.1.0",
  "description": "Runtime for executing ISL behaviors with contract enforcement",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run --passWithNoTests",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "test:unit": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "@isl-lang/isl-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "tsup": "^8.0.1",
    "rimraf": "^5.0.5"
  },
  "keywords": [
    "isl",
    "runtime",
    "contracts",
    "behaviors",
    "execution"
  ],
  "author": "mevla",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardiavault-oss/ISL-LANG.git"
  },
  "homepage": "https://intentlang.dev",
  "bugs": {
    "url": "https://github.com/guardiavault-oss/ISL-LANG/issues"
  },
  "private": true,
  "experimental": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
