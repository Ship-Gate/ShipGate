{
  "name": "@isl-lang/spec-inference",
  "version": "0.1.0",
  "description": "ISL Verify - Automatic behavioral spec inference from TypeScript/JavaScript codebases",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@isl-lang/parser": "workspace:*",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.5",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "isl",
    "verify",
    "spec-inference",
    "reverse-engineering",
    "code-analysis",
    "prisma",
    "zod",
    "nextjs",
    "express",
    "fastify"
  ],
  "license": "MIT",
  "author": "ISL Team",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardiavault-oss/ISL-LANG.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
