{
  "name": "@isl-lang/stdlib-http",
  "version": "0.1.0",
  "description": "HTTP/REST standard library for ISL",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./intents/*": "./intents/*"
  },
  "files": [
    "dist",
    "intents"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "keywords": [
    "isl",
    "http",
    "rest",
    "api",
    "stdlib"
  ],
  "author": "mevla",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardiavault-oss/ISL-LANG.git"
  },
  "homepage": "https://intentlang.dev",
  "bugs": {
    "url": "https://github.com/guardiavault-oss/ISL-LANG/issues"
  }
}
