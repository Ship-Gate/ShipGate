{
  "name": "@isl-lang/stdlib-files",
  "version": "1.0.0",
  "description": "File storage standard library for ISL - upload, download, manage files",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./s3": {
      "import": "./dist/s3.js",
      "types": "./dist/s3.d.ts"
    }
  },
  "files": [
    "dist",
    "intents"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/s3.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts src/s3.ts --format esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.500.0",
    "@aws-sdk/s3-request-presigner": "^3.500.0"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "@isl-lang/isl-core": "workspace:*"
  },
  "author": "mevla",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mevla/isl-lang.git"
  },
  "homepage": "https://intentlang.dev",
  "bugs": {
    "url": "https://github.com/mevla/isl-lang/issues"
  }
}
