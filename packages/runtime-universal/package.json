{
  "name": "@isl-lang/runtime-universal",
  "version": "1.0.0",
  "description": "Universal runtime engine for ISL - execute intent specifications across any environment",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./vm": {
      "types": "./dist/vm/index.d.ts",
      "import": "./dist/vm/index.mjs",
      "require": "./dist/vm/index.js"
    },
    "./scheduler": {
      "types": "./dist/scheduler/index.d.ts",
      "import": "./dist/scheduler/index.mjs",
      "require": "./dist/scheduler/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@isl-lang/isl-core": "workspace:*",
    "quickjs-emscripten": "^0.29.0"
  },
  "optionalDependencies": {
    "isolated-vm": "^4.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "isl",
    "runtime",
    "vm",
    "execution",
    "universal"
  ],
  "author": "mevla",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardiavault-oss/ISL-LANG.git"
  },
  "homepage": "https://intentlang.dev",
  "bugs": {
    "url": "https://github.com/guardiavault-oss/ISL-LANG/issues"
  }
}
