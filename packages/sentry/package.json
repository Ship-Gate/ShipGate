{
  "name": "@isl-lang/sentry",
  "version": "1.0.0",
  "description": "Sentry integration for ISL error tracking and performance monitoring",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    },
    "./integrations": {
      "types": "./dist/integrations/index.d.ts",
      "import": "./dist/integrations/index.mjs",
      "require": "./dist/integrations/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "import": "./dist/middleware/index.mjs",
      "require": "./dist/middleware/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "test:unit": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@sentry/node": "^8.0.0",
    "@sentry/profiling-node": "^8.0.0",
    "@sentry/types": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "tsup": "^8.0.0",
    "vitest": "^1.0.0",
    "rimraf": "^5.0.5"
  },
  "peerDependencies": {
    "@isl-lang/isl-core": "workspace:*",
    "@isl-lang/verifier-runtime": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@isl-lang/isl-core": {
      "optional": true
    },
    "@isl-lang/verifier-runtime": {
      "optional": true
    }
  },
  "keywords": [
    "sentry",
    "isl",
    "error-tracking",
    "performance-monitoring",
    "verification",
    "intentos"
  ],
  "author": "mevla",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardiavault-oss/ISL-LANG.git"
  },
  "homepage": "https://intentlang.dev",
  "bugs": {
    "url": "https://github.com/guardiavault-oss/ISL-LANG/issues"
  },
  "private": true,
  "experimental": true,
  "type": "module",
  "sideEffects": false
}
