{
  "name": "@isl-lang/codegen-property-tests",
  "version": "0.1.1",
  "description": "Generate property-based tests using fast-check from ISL specifications",
  "main": "dist/packages/codegen-property-tests/src/index.js",
  "types": "dist/packages/codegen-property-tests/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "test:unit": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "isl",
    "property-based-testing",
    "fast-check",
    "codegen",
    "invariants"
  ],
  "author": "mevla",
  "license": "MIT",
  "dependencies": {
    "@isl-lang/isl-core": "workspace:*",
    "@isl-lang/parser": "workspace:^",
    "fast-check": "^3.15.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "fast-check": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardiavault-oss/ISL-LANG.git"
  },
  "homepage": "https://intentlang.dev",
  "bugs": {
    "url": "https://github.com/guardiavault-oss/ISL-LANG/issues"
  },
  "private": true,
  "experimental": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
