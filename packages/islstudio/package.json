{
  "name": "@isl-lang/islstudio",
  "version": "0.1.2",
  "description": "Shipgate - Stop AI from shipping fake features",
  "type": "module",
  "bin": {
    "shipgate": "./bin/islstudio.js",
    "islstudio": "./bin/islstudio.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup src/index.ts --format esm --watch",
    "test": "vitest run --passWithNoTests",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@isl-lang/policy-packs": "workspace:*",
    "@isl-lang/firewall": "workspace:*",
    "@isl-lang/evidence": "workspace:*",
    "@isl-lang/proof": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "keywords": [
    "isl",
    "security",
    "gate",
    "verification",
    "ai-safety",
    "code-review"
  ],
  "author": "ISL Team",
  "license": "MIT",
  "homepage": "https://shipgate.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/shipgate/shipgate"
  },
  "private": true,
  "experimental": true
}
