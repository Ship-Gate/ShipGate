{
  "name": "@isl-lang/api-versioning",
  "version": "0.1.0",
  "description": "API versioning system that integrates with ISL domain versions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "test:unit": "vitest run --passWithNoTests"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./express": {
      "types": "./dist/middleware/express.d.ts",
      "import": "./dist/middleware/express.js",
      "require": "./dist/middleware/express.js"
    },
    "./fastify": {
      "types": "./dist/middleware/fastify.d.ts",
      "import": "./dist/middleware/fastify.js",
      "require": "./dist/middleware/fastify.js"
    }
  },
  "dependencies": {
    "semver": "^7.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/semver": "^7.5.6",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "tsup": "^8.0.1",
    "rimraf": "^5.0.5"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "isl",
    "api",
    "versioning",
    "migration",
    "compatibility"
  ],
  "author": "mevla",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardiavault-oss/ISL-LANG.git"
  },
  "homepage": "https://intentlang.dev",
  "bugs": {
    "url": "https://github.com/guardiavault-oss/ISL-LANG/issues"
  },
  "private": true,
  "experimental": true,
  "type": "module",
  "sideEffects": false
}
