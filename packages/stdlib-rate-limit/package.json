{
  "name": "@isl-lang/stdlib-rate-limit",
  "version": "2.0.0",
  "description": "ISL Standard Library - Rate Limiting and Throttling",
  "license": "MIT",
  "author": "IntentOS Team",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardiavault-oss/ISL-LANG.git"
  },
  "keywords": [
    "intentos",
    "isl",
    "stdlib",
    "rate-limit",
    "throttling",
    "brute-force",
    "protection",
    "ddos"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types.d.ts",
        "default": "./dist/types.js"
      },
      "require": {
        "types": "./dist/types.d.cts",
        "default": "./dist/types.cjs"
      }
    },
    "./storage/memory": {
      "import": {
        "types": "./dist/storage/memory.d.ts",
        "default": "./dist/storage/memory.js"
      },
      "require": {
        "types": "./dist/storage/memory.d.cts",
        "default": "./dist/storage/memory.cjs"
      }
    },
    "./storage/redis": {
      "import": {
        "types": "./dist/storage/redis.d.ts",
        "default": "./dist/storage/redis.js"
      },
      "require": {
        "types": "./dist/storage/redis.d.cts",
        "default": "./dist/storage/redis.cjs"
      }
    },
    "./adapters/express": {
      "import": {
        "types": "./dist/adapters/express.d.ts",
        "default": "./dist/adapters/express.js"
      },
      "require": {
        "types": "./dist/adapters/express.d.cts",
        "default": "./dist/adapters/express.cjs"
      }
    }
  },
  "files": [
    "dist",
    "intents"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest --coverage",
    "lint": "eslint implementations/typescript --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "test:unit": "vitest run --passWithNoTests"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "peerDependencies": {
    "@isl-lang/isl-core": "workspace:*",
    "ioredis": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "@isl-lang/isl-core": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://intentlang.dev",
  "bugs": {
    "url": "https://github.com/guardiavault-oss/ISL-LANG/issues"
  },
  "sideEffects": false
}
