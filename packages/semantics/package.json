{
  "name": "@isl-lang/semantics",
  "version": "0.1.0",
  "description": "Versioned semantics for ISL operators and clauses",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./adapter": {
      "types": "./dist/adapter.d.ts",
      "import": "./dist/adapter.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test:unit": "vitest run --passWithNoTests"
  },
  "dependencies": {},
  "devDependencies": {
    "typescript": "^5.4.0",
    "vitest": "^1.5.0",
    "tsup": "^8.0.1",
    "rimraf": "^5.0.5"
  },
  "keywords": [
    "isl",
    "semantics",
    "versioning",
    "operators"
  ],
  "license": "MIT",
  "sideEffects": false
}
