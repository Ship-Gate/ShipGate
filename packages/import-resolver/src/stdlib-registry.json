    },
    "@isl/url": {
      "version": "1.0.0",
      "path": "stdlib/url",
      "files": {
        "index": "index.isl"
      },
      "exports": ["URLString", "Protocol", "Host", "Port", "Path", "QueryString", "Fragment", "ParsedURL", "URLComponents", "QueryParams", "URLParseResult", "Parse", "TryParse", "ParseQuery", "GetProtocol", "GetHost", "GetHostname", "GetPort", "GetPathname", "GetSearch", "GetHash", "GetOrigin", "GetQueryParam", "GetQueryParams", "GetQueryParamAll", "HasQueryParam", "SetQueryParam", "SetQueryParams", "RemoveQueryParam", "ClearQueryParams", "Build", "BuildQuery", "Join", "Resolve", "IsValid", "IsAbsolute", "IsRelative", "IsHttps", "IsSameOrigin", "Normalize", "RemoveTrailingSlash", "AddTrailingSlash", "SetProtocol", "SetHost", "SetPathname", "SetHash", "GetPathSegments", "GetFilename", "GetExtension"],
      "description": "URL parsing and manipulation operations (100% deterministic)",
      "deterministic": true
    }
  },
  "aliases": {
    "stdlib-string": "@isl/string",
    "stdlib-strings": "@isl/string",
    "stdlib-math": "@isl/math",
    "stdlib-collections": "@isl/collections",
    "stdlib-json": "@isl/json",
    "stdlib-datetime": "@isl/datetime",
    "stdlib-uuid": "@isl/uuid",
    "stdlib-crypto": "@isl/crypto",
    "stdlib-encoding": "@isl/encoding",
    "stdlib-regex": "@isl/regex",
    "stdlib-url": "@isl/url"
  },
  "categories": {
    "core": {
      "name": "Core Primitives",
      "description": "Essential string, number, and collection operations",
      "modules": ["@isl/string", "@isl/math", "@isl/collections"]
    },
    "data": {
      "name": "Data Formats",
      "description": "JSON, encoding, and URL handling",
      "modules": ["@isl/json", "@isl/encoding", "@isl/url"]
    },
    "temporal": {
      "name": "Temporal",
      "description": "Date, time, and unique identifiers",
      "modules": ["@isl/datetime", "@isl/uuid"]
    },
    "security": {
      "name": "Security",
      "description": "Cryptographic operations",
      "modules": ["@isl/crypto"]
    },
    "patterns": {
      "name": "Patterns",
      "description": "Pattern matching and validation",
      "modules": ["@isl/regex"]
    }
  },
  "determinism_policy": {
    "version": "1.0.0",
    "description": "Functions marked as non-deterministic are excluded from formal verification",
    "fully_deterministic_modules": [
      "@isl/string",
      "@isl/math",
      "@isl/collections",
      "@isl/json",
      "@isl/encoding",
      "@isl/regex",
      "@isl/url"
    ],
    "mixed_determinism_modules": [
      "@isl/datetime",
      "@isl/uuid",
      "@isl/crypto"
    ],
    "nondeterministic_functions": {
      "@isl/datetime": ["Now"],
      "@isl/uuid": ["GenerateUUID", "GenerateUUIDv7"],
      "@isl/crypto": ["GenerateToken", "GenerateApiKey", "GenerateBytes", "HashPassword"]
    }
  }
}
