    "@isl/regex": {
      "version": "1.0.0",
      "path": "stdlib/regex",
      "files": {
        "index": "index.isl"
      },
      "exports": ["RegexPattern", "RegexFlags", "MatchResult", "Match", "MatchAllResult", "ReplaceResult", "SplitResult", "PatternInfo", "ValidationError", "Test", "Match", "MatchAll", "Exec", "Groups", "Captures", "Replace", "ReplaceAll", "ReplaceWithFunction", "Split", "Escape", "IsValidPattern", "ValidatePattern", "GetPatternInfo", "JoinPatterns", "WrapGroup", "Quantify", "MatchEmail", "MatchUrl", "MatchPhone", "MatchUuid", "MatchIpAddress", "ExtractAll", "ExtractNamed"],
