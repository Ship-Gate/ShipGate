{
  "name": "@isl-lang/isl-stdlib",
  "version": "1.0.0",
  "description": "ISL Standard Library Registry - Central index and resolver for all stdlib modules",
  "license": "MIT",
  "author": "IntentOS Team",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardiavault-oss/ISL-LANG.git"
  },
  "keywords": [
    "intentos",
    "isl",
    "stdlib",
    "registry",
    "import-resolver"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./registry": {
      "import": {
        "types": "./dist/registry.d.ts",
        "default": "./dist/registry.js"
      },
      "require": {
        "types": "./dist/registry.d.cts",
        "default": "./dist/registry.cjs"
      }
    },
    "./resolver": {
      "import": {
        "types": "./dist/resolver.d.ts",
        "default": "./dist/resolver.js"
      },
      "require": {
        "types": "./dist/resolver.d.cts",
        "default": "./dist/resolver.cjs"
      }
    },
    "./registry.json": "./registry.json"
  },
  "files": [
    "dist",
    "registry.json"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "generate-registry": "tsx src/generate-registry.ts",
    "validate-registry": "tsx src/validate-registry.ts",
    "test:unit": "vitest run --passWithNoTests"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "peerDependencies": {
    "@isl-lang/isl-core": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@isl-lang/isl-core": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://intentlang.dev",
  "bugs": {
    "url": "https://github.com/guardiavault-oss/ISL-LANG/issues"
  },
  "sideEffects": false
}
