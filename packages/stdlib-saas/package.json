{
  "name": "@isl-lang/stdlib-saas",
  "private": true,
  "version": "2.0.0",
  "description": "ISL Standard Library for SaaS applications - complete starter with auth, billing, dashboard",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "intents",
    "templates",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "test:unit": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@isl-lang/stdlib-auth": "workspace:*",
    "@isl-lang/stdlib-payments": "workspace:*",
    "@isl-lang/stdlib-core": "workspace:*",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1"
  },
  "peerDependencies": {
    "@isl-lang/isl-core": "workspace:*"
  },
  "keywords": [
    "intentos",
    "isl",
    "saas",
    "starter",
    "template",
    "stdlib"
  ],
  "author": "mevla",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardiavault-oss/ISL-LANG.git"
  },
  "homepage": "https://intentlang.dev",
  "bugs": {
    "url": "https://github.com/guardiavault-oss/ISL-LANG/issues"
  },
  "sideEffects": false
}
