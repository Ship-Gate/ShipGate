{
  "name": "@isl-lang/stdlib-scheduling",
  "version": "1.0.0",
  "description": "ISL Standard Library - Job Scheduling (cron, delayed jobs, workflows)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./intents/*": "./intents/*"
  },
  "files": [
    "dist",
    "intents"
  ],
  "scripts": {
    "build": "tsup implementations/typescript/index.ts --format esm --dts --clean --outDir dist",
    "dev": "tsup implementations/typescript/index.ts --format esm --dts --watch --outDir dist",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "test:unit": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "cron-parser": "^4.9.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "@types/uuid": "^9.0.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "keywords": [
    "isl",
    "stdlib",
    "scheduling",
    "cron",
    "jobs",
    "workflows",
    "queue"
  ],
  "author": "mevla",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardiavault-oss/ISL-LANG.git"
  },
  "homepage": "https://intentlang.dev",
  "bugs": {
    "url": "https://github.com/guardiavault-oss/ISL-LANG/issues"
  },
  "sideEffects": false
}
