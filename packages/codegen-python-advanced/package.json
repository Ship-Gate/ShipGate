{
  "name": "@isl-lang/codegen-python-advanced",
  "version": "0.1.0",
  "description": "Advanced Python code generation with FastAPI, Django, and SQLAlchemy support",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "test:unit": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "@isl-lang/parser": "workspace:*",
    "@isl-lang/typechecker": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "tsup": "^8.0.1",
    "rimraf": "^5.0.5"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "isl",
    "python",
    "fastapi",
    "django",
    "sqlalchemy",
    "pydantic",
    "codegen"
  ],
  "license": "MIT",
  "author": "mevla",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardiavault-oss/ISL-LANG.git"
  },
  "homepage": "https://intentlang.dev",
  "bugs": {
    "url": "https://github.com/guardiavault-oss/ISL-LANG/issues"
  },
  "private": true,
  "experimental": true,
  "sideEffects": false
}
