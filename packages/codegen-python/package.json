{
  "name": "@isl-lang/codegen-python",
  "version": "0.1.0",
  "description": "Python code generator for ISL",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "keywords": [
    "isl",
    "codegen",
    "python",
    "code-generation"
  ],
  "author": "mevla",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mevla/isl-lang.git"
  },
  "homepage": "https://intentlang.dev",
  "bugs": {
    "url": "https://github.com/mevla/isl-lang/issues"
  }
}
